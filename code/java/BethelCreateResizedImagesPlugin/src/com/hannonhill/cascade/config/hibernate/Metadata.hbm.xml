<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//END" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<union-subclass name="com.hannonhill.cascade.model.dom.Metadata" table="cxml_entitymetadata" extends="com.hannonhill.cascade.model.dom.BaseDomainObject">
		<property name="created" column="creationDate"/>
		<property name="modified" column="lastModifiedDate"/>
		<property name="createdById" column="createdBy"/>
		<property name="modifiedById" column="lastModifiedBy"/>
	</union-subclass>
</hibernate-mapping>