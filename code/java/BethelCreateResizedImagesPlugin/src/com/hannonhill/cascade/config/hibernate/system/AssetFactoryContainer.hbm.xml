<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//END" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<union-subclass name="com.hannonhill.cascade.model.dom.AssetFactoryContainer" table="cxml_assetfactorycontainer" extends="com.hannonhill.cascade.model.dom.SystemAreaEntityContainer">
		<many-to-one 
			name="parentContainer" 
			column="containerId"
			class="com.hannonhill.cascade.model.dom.AssetFactoryContainer"
		/>
		<set name="childContainers" inverse="true" cascade="all">
			<key column="containerId"/>
			<one-to-many class="com.hannonhill.cascade.model.dom.AssetFactoryContainer"/>
		</set>
		<set name="children" inverse="true" cascade="all">
			<key column="containerId"/>
			<one-to-many class="com.hannonhill.cascade.model.dom.AssetFactory"/>
		</set>
		<property name="groups" type="text"/>
	</union-subclass>
</hibernate-mapping>